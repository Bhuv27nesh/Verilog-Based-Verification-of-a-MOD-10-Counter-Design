v0x5640dd805250_0 .net "rst", 0 0, v0x5640dd805900_0;  1 drivers
E_0x5640dd7e03b0/0 .event negedge, v0x5640dd805250_0;
E_0x5640dd7e03b0/1 .event posedge, v0x5640dd7d58d0_0;
E_0x5640dd7e03b0 .event/or E_0x5640dd7e03b0/0, E_0x5640dd7e03b0/1;
    .scope S_0x5640dd7e1f20;
T_0 ;
    %wait E_0x5640dd7e03b0;
    %load/vec4 v0x5640dd805250_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5640dd804ff0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5640dd805190_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5640dd8050d0_0;
    %cmpi/u 10, 0, 4;
    %jmp/0xz  T_0.4, 5;
    %load/vec4 v0x5640dd8050d0_0;
    %assign/vec4 v0x5640dd804ff0_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5640dd804ff0_0, 0;
T_0.5 ;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5640dd804ff0_0;
    %cmpi/u 9, 0, 4;
    %jmp/0xz  T_0.6, 5;
    %load/vec4 v0x5640dd804ff0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x5640dd804ff0_0, 0;
    %jmp T_0.7;
T_0.6 ;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5640dd804ff0_0, 0;
T_0.7 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5640dd7d6570;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x5640dd805400_0;
    %inv;
    %store/vec4 v0x5640dd805400_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5640dd7d6570;
T_2 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5640dd805750_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5640dd8054c0_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5640dd805400_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5640dd805900_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5640dd805860_0, 0, 1;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x5640dd805680_0, 0, 4;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5640dd805900_0, 0, 1;
    %delay 130, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5640dd805900_0, 0, 1;
    %pushi/vec4 150, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 30, 0;
    %load/vec4 v0x5640dd805860_0;
    %inv;
    %store/vec4 v0x5640dd805860_0, 0, 1;
    %delay 10, 0;
    %load/vec4 v0x5640dd805680_0;
    %addi 1, 0, 4;
    %store/vec4 v0x5640dd805680_0, 0, 4;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %pushi/vec4 200, 0, 32;
T_2.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.3, 5;
    %jmp/1 T_2.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_func 2 40 "$random" 32 {0 0 0};
    %pad/s 1;
    %store/vec4 v0x5640dd805900_0, 0, 1;
    %vpi_func 2 41 "$random" 32 {0 0 0};
    %pad/s 1;
    %store/vec4 v0x5640dd805860_0, 0, 1;
    %vpi_func 2 42 "$random" 32 {0 0 0};
    %pad/s 4;
    %store/vec4 v0x5640dd805680_0, 0, 4;
    %delay 20, 0;
    %jmp T_2.2;
T_2.3 ;
    %pop/vec4 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5640dd805900_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 46 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5640dd7d6570;
T_3 ;
