T_4.7 ;
T_4.4 ;
    %load/vec4 v0x5640dd805860_0;
    %cmpi/e 1, 0, 1;
    %flag_get/vec4 4;
    %jmp/0 T_4.10, 4;
    %pushi/vec4 10, 0, 4;
    %load/vec4 v0x5640dd805680_0;
    %cmp/u;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %and;
T_4.10;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.8, 8;
    %load/vec4 v0x5640dd805580_0;
