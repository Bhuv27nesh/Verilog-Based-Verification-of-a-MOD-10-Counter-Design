    %vpi_call 2 52 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5640dd7d6570 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5640dd7d6570;
T_4 ;
    %wait E_0x5640dd7e03b0;
    %load/vec4 v0x5640dd805900_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_4.0, 4;
    %load/vec4 v0x5640dd805580_0;
