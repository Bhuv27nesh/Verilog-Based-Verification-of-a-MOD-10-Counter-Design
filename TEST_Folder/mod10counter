    %vpi_call 2 81 "$display", "ERROR!!! Testcase 3_1: Load high , din>10, count == 4'b0" {0 0 0};
T_4.12 ;
    %jmp T_4.9;
T_4.8 ;
    %load/vec4 v0x5640dd805860_0;
