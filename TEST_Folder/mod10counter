v0x5640dd805900_0 .var "rst", 0 0;
S_0x5640dd7e1f20 .scope module, "dut" "mod10counter" 2 17, 3 35 0, S_0x5640dd7d6570;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "count";
    .port_info 3 /INPUT 1 "load";
    .port_info 4 /INPUT 4 "din";
v0x5640dd7d58d0_0 .net "clk", 0 0, v0x5640dd805400_0;  1 drivers
v0x5640dd804ff0_0 .var "count", 3 0;
v0x5640dd8050d0_0 .net "din", 3 0, v0x5640dd805680_0;  1 drivers
