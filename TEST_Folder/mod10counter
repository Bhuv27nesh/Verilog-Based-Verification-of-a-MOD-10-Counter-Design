S_0x5640dd7d6570 .scope module, "mod10counter_test" "mod10counter_test" 2 3;
 .timescale -9 -9;
v0x5640dd805400_0 .var "clk", 0 0;
v0x5640dd8054c0_0 .var/i "correct", 31 0;
